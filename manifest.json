{
  "name": "Ulaa Dashboard",
  "description": "New tab dashboard showing bookmarks, most visited, and recent sites.",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Ulaa Dashboard"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "bookmarks",
    "history",
    "topSites",
    "sessions"
  ]
}
